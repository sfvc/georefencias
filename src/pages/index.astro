---
// Import the required React component
import { Mapa } from 'component/mapa';
import proyectos from '../data/proyectos.json';
import Layout from './layout/Layout.astro';

// Find the "sfcv" project from the data
const proyecto = proyectos.find((btn) => btn.nombre === "sfvc");

// If no project is found, you might want to handle that error
if (!proyecto) {
  throw new Error("Proyecto 'sfcv' no encontrado");
}
---
<Layout title="SFCV georeferenciado">
  <Mapa proyecto={proyecto} client:only="react" />
</Layout>

